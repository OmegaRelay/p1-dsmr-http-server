
/ {
    chosen {
        zephyr,p1-uart = &uart1;
    };

    aliases {
        led0 = &led0;
    };

    outputs {
		compatible = "gpio-leds";
		led0: led0 {
			gpios = <&gpio0 2 (GPIO_ACTIVE_HIGH | GPIO_PUSH_PULL)>;
		};
	};
};

&uart1 {
    status = "okay";
    current-speed = <115200>;
	pinctrl-0 = <&uart1_default>;
	pinctrl-names = "default";
};

&wifi {
	status = "okay";
};
